        //Constructor with 4 parameters
public Dog(String name, String breed, float weight, int sex) {

        this.name=name;
        this.breed=breed;
        this.weight=weight;
        this.sex=sex;

        }
  //Default Constructor
  public Dog() {     

        }
        
        //Constructor with 3 parameters
public Dog(String name,  float weight, int sex) {

        this.name=name;
        this.weight=weight;
        this.sex=sex;

        }
        
//Constructor with 2 parameters
public Dog(float weight, int sex) {

        this.weight=weight;
        this.sex=sex;

        }

3	interface Shape{

    void draw();
 }

abstract class GenericShape{

    void rotate(int degree) {
        System.out.print(""Rotates "" + degree + "" degrees"");
    }


}


class Rectangle extends GenericShape implements shape {
    void draw() {
        System.out.print(""draws a rectangle"");
    }


}

class Circle extends GenericShape implements shape {
    void draw() {
        System.out.print(""draws a circle"");
    }


}

